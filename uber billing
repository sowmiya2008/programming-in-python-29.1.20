def billing(dest):
  rate=dest*10
  print("the cost of the destination is ",rate)

a=int(input("enter the destinatio \n 1.avadi\n 2.maduravoyal\n 3.t.nagar"))
if(a==1):
    billing(20)
elif(a==2):
    billing(30)
elif(a==3):
    billing(50)
else:
    print("wrong destination")

  
